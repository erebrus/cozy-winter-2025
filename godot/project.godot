config_version=5

[application]

config/name="Chrysalis Cafe"
config/version="0.0.12"
run/main_scene="res://src/start_screen/start_screen.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Types="*res://src/common/types.gd"
Events="*res://src/common/events.gd"
Globals="*res://src/common/globals.tscn"
Logger="*res://addons/gs_logger/logger.gd"
Debug="*res://src/common/debug/debug.tscn"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
Narrative="*res://src/narrative/narrative.tscn"
HyperLog="*res://addons/hyperlog/hyperlog.tscn"

[dialogue_manager]

general/states=["Narrative"]
general/wrap_lines=true
general/balloon_path="res://src/dialogue/balloon.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg", "res://addons/gs_logger/plugin.cfg", "res://addons/hyperlog/plugin.cfg", "res://addons/random_timers/plugin.cfg", "res://addons/script-ide/plugin.cfg", "res://addons/xsm/plugin.cfg")

